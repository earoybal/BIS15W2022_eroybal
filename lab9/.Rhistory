library(tidyverse)
library(janitor)
library(here)
library(naniar)
colleges <- read.csv('data/ca_college_data.csv')
colnames(colleges)
glimpse(colleges)
any.na(colleges)
any_na(colleges)
colleges %>%
summarize_all(~is.na(.))
colleges %>%
summarize_all(sum(~is.na(.)))
colleges %>%
summarize_all(sum(~is.na(.)))
colleges %>%
summarize_all(~sum(is.na(.)))
colleges <- janitor::clean_names(colleges)
colnames(colleges)
colleges %>%
group_by(city) %>%
summarise(n())
colleges %>%
group_by(city) %>%
summarise(n()) %>%
arrange(desc())
colleges %>%
group_by(city) %>%
summarise(num_colleges = n()) %>%
arrange(desc())
colleges %>%
group_by(city) %>%
summarise(num_colleges = n()) %>%
arrange(desc(num_colleges))
num_colleges <- colleges %>%
group_by(city) %>%
summarise(num_colleges = n()) %>%
arrange(desc(num_colleges))
num_colleges
num_colleges %>%
ggplot(aes(x = city, y = num_colleges)) +
geom_col()
num_colleges %>%
top_n(10, num_colleges) %>%
ggplot(aes(x = city, y = num_colleges)) +
geom_col()
num_colleges %>%
top_n(10, num_colleges) %>%
ggplot(aes(x = city, y = num_colleges)) +
geom_col() +
coord_flip()
colnames(colleges)
